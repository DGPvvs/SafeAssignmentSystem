@model string

@{
    ViewBag.Title = "Избор на файл, съдържащ сменен график за месец";
}

<h2 class="text-center">@ViewBag.Title</h2>
<hr />

<form method="post" enctype="multipart/form-data" asp-controller="WorkingRotation" asp-action="LoadFromFile">
    <div class="input-group mb-3">
        <div class="input-group-prepend">
            <button class="btn btn-success" type="submit" id="inputGroupFileAddon03" value="Upload">Трансфер на файл</button>
        </div>

        <div class="custom-file">
            <input type="file" name="file" class="btn-warning" id="inputGroupFile03" aria-describedby="inputGroupFileAddon03">
            <label class="custom-file-label" for="inputGroupFile03"></label>
        </div>
    </div>
</form>
